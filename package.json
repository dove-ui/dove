{
  "name": "dove",
  "version": "0.1.0",
  "description": "A Vue UI Toolkit for Web ",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/dove-ui/dove",
  "repository": {
    "type": "https",
    "url": "git+https://github.com/dove-ui/dove"
  },
  "bugs": {
    "url": "https://github.com/dove-ui/dove/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "github-markdown-css": "^3.0.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue": "^2.6.10",
    "vue-dotmd-loader": "^0.1.0",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "**/*.md"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
